<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TextHeader" parent="Theme.Todo">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:fontFamily">@font/font_inter_bold</item>
        <item name="android:includeFontPadding">false</item>
    </style>
    <style name="TextValue" parent="TextHeader">
        <item name="android:fontFamily">@font/font_inter_medium</item>
    </style>
    <style name="Chips">
        <item name="android:checkable">true</item>
        <item name="checkedIconVisible">false</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="textStartPadding">20dp</item>
        <item name="textEndPadding">20dp</item>
        <item name="chipBackgroundColor">@color/enable_bottom_sheet_chips_background_color</item>
        <item name="chipStrokeColor">@color/black</item>
        <item name="chipStrokeWidth">1dp</item>
        <item name="android:textColor">@color/enable_bottom_sheet_text_color</item>
    </style>
    <style name="Okbtn">
        <item name="android:textSize">@dimen/b1_text_size</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:letterSpacing">0</item>
        <item name="backgroundTint">@color/black</item>
        <item name="rippleColor">@color/ripple_black</item>
        <item name="cornerRadius">20dp</item>
        <item name="android:text">@string/ok</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">258dp</item>
        <item name="android:layout_marginBottom">40dp</item>
    </style>
    <style name="CheckBox">
        <item name="android:layout_weight">1</item>
        <item name="minWidth">0dp</item>
        <item name="minHeight">0dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:button">@null</item>
        <item name="layout_constraintTop_toTopOf">parent</item>
    </style>
</resources>